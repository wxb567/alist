{
  "builds": [
    {
      "src": "api/alist.go",
      "use": "@vercel/go",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": [
          "alist",
          "config.json",
          "data/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/alist"
    }
  ],
  "build": {
    "env": {
      "GO111MODULE": "off",
      "CGO_ENABLED": "0",
      "GOOS": "linux",
      "GOARCH": "amd64"
    }
  }
}